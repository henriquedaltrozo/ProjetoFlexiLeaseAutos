                  "example-1": {
                    "value": [
                      {
                        "start_date": "01/01/2021",
                        "end_date": "30/01/2021",
                        "id_car": "668ae8ea455ddc08388453e0",
                        "id_user": "668a238866297b9bbc27ad77",
                        "final_value": 1015,
                        "_id": "668aee9d455ddc083884543c"
                      }
                    ]
                  }
                }
              }
            }
          }
